<Window x:Class="WpfVending.Windows.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfVending.Windows"
        mc:Ignorable="d"
        Loaded="Window_Loaded"
        Title="Vending Admin" Height="450" Width="800">
    <StackPanel Margin="10">
        <DataGrid Margin="10" Name="Products"/>
        <DataGrid Margin="10" IsReadOnly="True" Name="Transactions" />
    </StackPanel>
</Window>
